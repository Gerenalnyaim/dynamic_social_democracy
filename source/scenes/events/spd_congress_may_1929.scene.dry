on-arrival: {!
Q.con_rural_strength = 0;
Q.con_middle_strength = 0;
Q.con_neorev_strength = 0;
Q.con_left_strength = 2 + (Q.left_strength / 8);
Q.con_center_strength = 2 + (Q.center_strength / 8);
Q.con_reform_strength = 2 + (Q.reformist_strength / 8);
Q.con_llabor_strength = 0.5 + ((Q.labor_strength / 18) * (Q.left_strength / Q.reformist_strength));
Q.con_rlabor_strength = 1 + ((Q.labor_strength / 14) * (Q.reformist_strength / Q.left_strength));
if (Q.rural_spd_normalized >= 12) {
Q.con_rural_strength = Q.rural_spd_normalized/3;
}
if (Q.old_middle_spd_normalized + Q.new_middle_spd_normalized >= 22) {
Q.con_middle_strength = (Q.old_middle_spd_normalized + Q.new_middle_spd_normalized) / 10;
}
if (Q.neorevisionism == 1) {
Q.con_neorev_strength = 2 + (Q.neorevisionist_strength/10);
}

let keys = [
    "con_left_strength",
    "con_center_strength",
    "con_reform_strength",
    "con_llabor_strength",
    "con_rlabor_strength",
    "con_rural_strength",
    "con_middle_strength",
    "con_neorev_strength"
];

let variables = keys.map(k => Q[k] ?? 0);

let total = variables.reduce((sum, val) => sum + val, 0);

let floored;

if (total === 0) {
    floored = Array(8).fill(Math.floor(397 / 8));
    let remainder = 100 - floored.reduce((a, b) => a + b, 0);
    for (let i = 0; i < remainder; i++) {
        floored[i]++;
    }
} else {
    let scaled = variables.map(v => (v / total) * 397);
    floored = scaled.map(Math.floor);
    let remainder = 397 - floored.reduce((sum, val) => sum + val, 0);
    let decimalParts = scaled.map((v, i) => ({ index: i, decimal: v - floored[i] }));
    decimalParts.sort((a, b) => b.decimal - a.decimal);
    for (let i = 0; i < remainder; i++) {
        floored[decimalParts[i].index]++;
    }
}

keys.forEach((k, i) => Q[k] = floored[i]);
[
    Q.con_left_strength,
    Q.con_center_strength,
    Q.con_reform_strength,
    Q.con_llabor_strength,
    Q.con_rlabor_strength,
    Q.con_rural_strength,
    Q.con_middle_strength,
    Q.con_neorev_strength
] = floored;

!}
